{
  "name": "telegram-transformer",
  "version": "1.1.0",
  "description": "Telegram message transformer",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "test": "jest --config jestconfig.json --passWithNoTests",
    "build": "tsc",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "prepare": "npm run build",
    "lint": "tslint -p tsconfig.json",
    "version": "npm run format && git add -A src",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "postversion": "git push && git push --tags"
  },
  "type": "commonjs",
  "author": "Ivan Odyntsov <ivan@odintsov.me>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/ivanodintsov/telegram-transformer.git"
  },
  "bugs": {
    "url": "https://github.com/ivanodintsov/telegraf/telegram-transformer"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "typegram": "^4.1.1"
  },
  "devDependencies": {
    "@types/jest": "^29.2.4",
    "jest": "^29.3.1",
    "prettier": "^2.8.1",
    "ts-jest": "^29.0.3",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.9.4"
  },
  "keywords": [
    "telegram-transformer",
    "telegram",
    "typegram",
    "telegram parse",
    "bot",
    "telegram entity",
    "telegram formatter"
  ]
}
